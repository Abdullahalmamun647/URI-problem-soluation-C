// problem 1067 = 	Odd Numbers
// Programmed by: Abdullah Al Mamun

// Lets's start..

#include <stdio.h>
#include <stdlib.h>

int main ()
{
    int n,i;
    scanf("%d",&n);
    if (n>=1 && n<=1000)
    {
        for(i=0;i<=n;i++)
            if(i%2==1 || i%2==-1)
                printf("%d\n",i);
    }
    return 0;
}
