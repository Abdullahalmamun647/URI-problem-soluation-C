// problem 1015= Distance Between Two Points
// Programmed by: Abdullah Al Mamun

// Lets's start..
#include <stdio.h>
#include <math.h>
#include <stdlib.h>
int main ()
{
    double x1,x2,y1,y2,M;
    
    scanf("%lf %lf",&x1,&y1);
    scanf("%lf %lf",&x2,&y2);
    
    
    M=pow((x2-x1), 2)+pow((y2-y1), 2);
    M=sqrt(M);
    printf("%.4f\n",M);

    
    return 0;
}
