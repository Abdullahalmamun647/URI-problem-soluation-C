// problem 1012= Area
// Programmed by: Abdullah Al Mamun

// Lets's start..

#include <stdio.h>
#include <math.h>

int main ()
{
    double A,B,C,triangle,circle,trapezium,square,rectangle;
    
    scanf("%lf",&A);
    scanf("%lf",&B);
    scanf("%lf",&C);
   
    
    triangle=0.5*(A*C);
    circle=3.14159*(C*C);
    trapezium=0.5*(A+B)*C;
    square=B*B;
    rectangle=A*B;
    
    printf("TRIANGULO: %.3f\n",triangle);
    printf("CIRCULO: %.3f\n",circle);
    printf("TRAPEZIO: %.3f\n",trapezium);
    printf("QUADRADO: %.3f\n",square);
    printf("RETANGULO: %.3f\n",rectangle);
    
    return 0;
}
