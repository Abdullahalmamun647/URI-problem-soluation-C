// Lets's start..

#include <stdio.h>
#include <stdlib.h>


int main ()
{
    int a,b;
    scanf("%d %d",&a,&b);
    if (a==b){
        printf("O JOGO DUROU %d HORA(S)\n",24-a+b);
    }
    else if (a>b){
          printf("O JOGO DUROU %d HORA(S)\n",24-(a-b));
    }
    else if (a<b){
        printf("O JOGO DUROU %d HORA(S)\n",b-a);

    }
    return 0;
}

