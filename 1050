// problem 1045 = DDD
// Programmed by: Abdullah Al Mamun

// Lets's start..
#include <stdio.h>
#include <stdlib.h>


int main ()
{
    int code;
    scanf("%d",&code);
    
    switch (code){
        case 61:
            printf("Brasilia\n");
            break;
        case 71:
            printf("Salvador\n");
            
            break;
        case 11:
            printf("Sao Paulo\n");
            break;
        case 21:
            printf("Rio de Janeiro\n");
            break;
        case 32:
            printf("Juiz de Fora\n");
            
            break;
        case 19:
            printf("Campinas\n");
            
            break;
        case 27:
            printf("Vitoria\n");
            break;
        case 31:
            printf("Bela Horizonte\n")
            break
            
            
        default:
            printf("DDD nao cadastrado\n");
            
    }
    
    return 0;
    
}

