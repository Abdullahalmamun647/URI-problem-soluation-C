  
// problem 1037 = Interval
// Programmed by: Abdullah Al Mamun

// Lets's start..


#include <stdio.h>
int main (){
    
    double M;
    
    scanf("%lf",&M);
    
    if (M>=0 && M<=25){
        
        printf("Intervalo [0,25]\n");
    }
    else if (M>25 && M<=50)
    {
        printf("Intervalo (25,50]\n");
    }
    else if (M>50 && M<=75){
        printf("Intervalo (50,75]\n");
    }
    else if(M>75 && M<=100)
    {
        printf("Intervalo (75,100]\n");
    }
    else{
        printf("Fora de intervalo\n");
    }
    return 0;
    
}
