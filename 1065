// problem 1065 = Even Between five Numbers
// Programmed by: Abdullah Al Mamun

// Lets's start..

#include <stdio.h>
#include <stdlib.h>

int main ()
{
    int i,n,counter = 0;
    for (i=0;i<5;i++)
    {
        scanf("%d",&n);
                if(n%2==0)
        {  counter ++;
                }
    }
 
    printf("%d valores pares\n",counter);

    return 0;
}
