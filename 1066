// problem 1053 = Even, Odd, Positive and Negative
// Programmed by: Abdullah Al Mamun

// Lets's start..

#include <stdio.h>
#include <stdlib.h>

int main ()
{
    int i,n,e = 0,o=0,p=0,N=0;
    for (i=0;i<5;i++)
    {
        scanf("%d",&n);
        if(n%2 !=0)
            o++;
        
        if(n%2==0)
            e ++;
        if (n>0)
            p++;
        if(n<0)
            N++;
        
    }
    
    printf("%d valor(es) par(es)\n",e);
    printf("%d valor(es) impar(es)\n",o);
    printf("%d valor(es) positivo(s)\n",p);
    printf("%d valor(es) negativo(s)\n",N);
    return 0;
}
